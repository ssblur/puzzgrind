pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
game = 0
w = 7 -- board width
h = 10 -- board height

#include ./characters.lua
#include ./board.lua
#include ./char.lua
#include ./points.lua
#include ./highscore.lua
#include ./story.lua
#include ./scene.lua

_bup = 4
_gup = 40
_ihi = false
function _update()
	_gup -= 1
	_gup %= 40
	if game==1 then
		_uchar()
		_ucrane()
		_upoint()
		_bup -= 1
		if _bup > 0 then
			return
		end
		_bup = 4
		_uboard()
	elseif game==0 then
		_ummenu()
	elseif game==2 then
		_cdiap = min(_cdiap+1, 
			#_cdial)
		if _x() then
			game = 0
		end
	elseif game==3 or game==5 then
		_upsel()
	elseif game==4 then
		_uscores()
	elseif game==6 then
		_uscene()
	elseif game==7 then
		_ufin()
	end
end

function _draw()
	cls()
	if game==1 then
		_dboard()
		_dchar()
		_dcrane()
		_dpoint()
	elseif game==0 then
		_dmmenu()
	elseif game==2 then
		local x = 64 - 7*4
		local y = 72 - 3
		local ch = _ch - 1
		ch *= 4
		map(2+ch,0,16,y-32,2,2)
		local d = 
		sub(_cdial, 0, _cdiap)
		print(d, 34, y-32, 7)
		rectfill(0, y-2, 127, y+19, 8)
		print("game over",
			x, y, 7)
		print("points: ".._points
			..(_ihi and " (high)" or ""))
		print("press ‚ùé...")
	elseif game==3 or game==5 then
		_dpsel()
	elseif game==4 then
		_dscores()
	elseif game==6 then
		_dscene()
	elseif game==7 then
		_dfin()
	end
end

