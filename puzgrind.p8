function _dmmenu()
	local x = _mmx
	local y = _mmy
	map(0, 2, x, y, 5, 3)
	
	y += 16
	x += 24
	print("", x, y, 7)
	for k,v in pairs(_mmopt) do
		print(v[1])
	end
	x -= 6
	print(">", x, y+_mmsel*6)
	
	x += 46
	y += 6
	print("code: blur", x, y, 13)
	print("art:  kaydio")
	
	map(0, 6, 0, 0, 16, 8)
end
