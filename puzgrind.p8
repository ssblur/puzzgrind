function _upsel()
	if _x() then
		game = 1
		dstart()
	end
	if mcool <= 0 then
		if _r() then
			mcool = 5
			_ch += 1
			sfx(2, 1)
		end
		if _l() then
			mcool = 5
			_ch -= 1
			sfx(2, 1)
		end
	else
		mcool -= 1
	end
	_ch = min(_ch, #c)
	_ch = max(_ch, 1)
end

function _dpsel()
 local x = 24
 local y = 40
	if _ch > 1 then
		spr(35, x, y, 1, 1, true)
	end
	
	x += 8
	local ch = _ch - 1
	ch *= 4
	map(
		(_gup>20 and 2 or 0) 
		+ ch, 0, 
		x,	y-4, 
		2, 2
	)
	
	print(c[_ch].name,x+24,y,7)
	print(c[_ch].gang,x+24,y+6,6)
	
	x += 16
	if _ch < #c then
		spr(35, x, y, 1, 1, false)
	end
	
	x -= 20
	y += 20
	print("choose a character",
		x, y, 7)
	print("\*5 \-d❎ to play")
end

function _x()
	return btnp(❎) or btnp(🅾️)
end

function _u()
	return btnp(⬆️)
end

function _d()
	return btnp(⬇️)
end

function _l()
	return btnp(⬅️)
end

function _r()
	return btnp(➡️)
end

