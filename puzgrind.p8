-- main menu

_mmsel = 1 -- menu selection
_mmopt = { -- menu options
	{
		"endless",
		function() 
			resetboard()
			game = 3
		end
	},
	{
		"story",
		function()
			game = 5
			_ch = 1
			_ptt = 0
		end
	},
	{
		"scores",
		function() 
			game = 4
		end
	},
	{
		"exit",
		function()
			cls()
			stop()
		end
	}
}
-- menu draw coords
_mmx = 8
_mmy = 72

-- update main menu
function _ummenu()
	if btnp(‚¨ÜÔ∏è) then
		_mmsel -= 1
		sfx(2, 1)
	end
	if btnp(‚¨áÔ∏è) then
		_mmsel += 1
		sfx(2, 1)
	end
	_mmsel = min(_mmsel, #_mmopt)
	_mmsel = max(_mmsel, 1)
	if _x() then
		_mmopt[_mmsel][2]()
	end
	
	local x = flr(rnd(16))
	local y = flr(rnd(8))+6
	local t = flr(rnd(5))+1
	mset(x, y, t)
end

-- draw main menu
function _dmmenu()
	local x = _mmx
	local y = _mmy
	map(0, 2, x, y, 5, 3)
	
	y += 16
	x += 24
	print("", x, y, 7)
	for k,v in pairs(_mmopt) do
		print(v[1])
	end
	x -= 6
	print(">", x, y+_mmsel*6)
	
	x += 46
	y += 6
	print("code: blur", x, y, 13)
	print("art:  kaydio")
	
	map(0, 6, 0, 0, 16, 8)
end
-->8
-- character select

-- update char select
function _upsel()
	if _x() then
		if game==5 then
			game = s[_ch].stages[1].t
			_sact = _ch
			_ch = s[_ch].char
		else
			game = 1
		end
		dstart()
	end
	if mcool <= 0 then
		if btnp(‚û°Ô∏è) then
			mcool = 5
			_ch += 1
			sfx(2, 1)
		end
		if btnp(‚¨ÖÔ∏è) then
			mcool = 5
			_ch -= 1
			sfx(2, 1)
		end
	else
		mcool -= 1
	end
	if game==5 then
		_ch = min(_ch, #s)
	else
		_ch = min(_ch, #c)
	end
	_ch = max(_ch, 1)
end

-- draw char select
function _dpsel()
 local x = 24
 local y = 40
	if _ch > 1 then
		spr(35, x, y, 1, 1, true)
	end
	
	x += 8
	local ch = _ch - 1
	if game==5 then
		ch = s[_ch].char - 1
	end
	ch *= 4
	map(
		(_gup>20 and 2 or 0) 
		+ ch, 0, 
		x,	y-4, 
		2, 2
	)
	
	print(c[_ch].name,x+24,y,7)
	print(c[_ch].gang,x+24,y+6,6)
	
	x += 16
	if _ch < #c then
		spr(35, x, y, 1, 1, false)
	end
	
	x -= 20
	y += 20
	print("choose a character",
		x, y, 7)
	print("\*5 \-d‚ùé to play")
	if game==5 then
		print("")
		print("story:")
		print('\t"'..s[_ch].name..'"')
	end
end

_xdel = 0
-- button helper
function _x()
	if _xdel >= 0 then -- delay next input
		_xdel -= 1
		return
	end
	if btnp(‚ùé) or btnp(üÖæÔ∏è) then
		sfx(2, 1)
		return true
	end
end

